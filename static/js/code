var bubbleData = [{ 
            x: `${filterData.otu_ids.slice(0,10).reverse()}`,
            y: filterData.sample_values.slice(0,10).reverse(),
            mode: 'markers'

            markers = {
                [filterData.sample_values.slice(0,10).reverse()]
            }

        }];

        var layout = {
            title: "Belly Button"
        };

        Plotly.newPlot("bubble", bubbleData, layout)
    

     });